{
    "name": "tiknil/wire-table",
    "description": "Livewire helpers and components to create tables in your laravel application",
    "keywords": [
        "tiknil",
        "wire-table"
    ],
    "version": "0.0.1",
    "homepage": "https://github.com/tiknil/wire-table",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Giorgio Balduzzi",
            "email": "balduzzi.giorgio@tiknil.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0",
        "livewire/livewire": "^2.12"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tiknil\\WireTable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tiknil\\WireTable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tiknil\\WireTable\\WireTableServiceProvider"
            ],
            "aliases": {
                "WireTable": "Tiknil\\WireTable\\WireTableFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
